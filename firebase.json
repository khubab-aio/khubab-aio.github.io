{
  "hosting": {
    "public": "/",
    "rewrites": [
      {
        "source": "/LoginWithEmail",
        "destination": "/LoginWithEmail.html"
      }
    ],
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "headers": [
      {
        "source": "/.well-known/apple-app-site-association",
        "headers": [{ "key": "Content-Type", "value": "application/json" }]
      }
    ]
  }
}
